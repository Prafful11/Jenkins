# Backend Environment Variables

# Server
PORT=5000

# MongoDB Database (you should have Mongo running, usually inside docker-compose)
MONGO_URL=mongodb://mongo:27017/jenkinsDB

# Redis Cache (for sessions, tokens, etc.)
REDIS_URL=redis://redis:6379

# Authentication Secret Keys
JWT_SECRET=your_jwt_secret_key_here
REFRESH_TOKEN_SECRET=your_refresh_token_secret_here

# Node Environment
NODE_ENV=production
